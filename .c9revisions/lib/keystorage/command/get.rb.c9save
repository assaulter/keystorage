{"ts":1348153225959,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"require 'keystorage/command/base'\n\nmodule Keystorage\n  module Command\n    class Get < Base\n      def execute(argv)\n        group = argv.shift\n        raise \"missing parameter\"  unless group\n        \n        name = argv.shift\n        ks = Manager.new(@file)\n        data = ks.get(group,name)\n        raise \"missing %s \" % [group] unless data\n        puts data\n      end\n\n      class << self\n        def help\n          \"Description:\n    get key from file\n\nUsage:\n    keystrage [options] get groups [name]\n\"\n        end\n      end\n    end\n  end\nend\n"]],"start1":0,"start2":0,"length1":0,"length2":545}]],"length":545}
